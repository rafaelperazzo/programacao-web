O:8:"stdClass":22:{s:2:"id";s:2:"36";s:8:"category";s:2:"10";s:6:"parent";s:1:"0";s:4:"name";s:26:"Desenvolvedores de Jogos 2";s:12:"questiontext";s:1267:"<div data-canvas-width="804.0006700000002">Cinco amigos: Bia, Carla, Dino, Edu e Flora, resolveram desenvolver um novo jogo de computador. As</div>
<div data-canvas-width="804.0006700000002">tarefas foram divididas em: Arte, Programação, Roteiro, Sons e Testes. Cada um vai ser responsável</div>
<div data-canvas-width="730.5642451666669">por uma única tarefa, mas algumas condições devem ser obedecidas na divisão das tarefas:</div>
<div data-canvas-width="258.09112416666665">1. Edu não trabalha em Roteiro.</div>
<div data-canvas-width="364.25484900000004">2. Bia só trabalha em Programação ou Testes.</div>
<div data-canvas-width="781.2733783333333">3. Flora trabalha em qualquer tarefa, mas se Carla trabalhar em Arte, Flora vai trabalhar em</div>
<div data-canvas-width="110.92736516666666">Programação.</div>
<div data-canvas-width="468.47311766666667">4. Dino trabalha somente em Roteiro ou em Programação.</div>
<div data-canvas-width="468.47311766666667"> </div>
<div data-canvas-width="468.47311766666667">
<div data-canvas-width="702.3824035000002">Se Carla trabalhar em Arte, então qual das alternativas abaixo são tarefas que Dino pode</div>
<div data-canvas-width="193.12743366666663">escolher para trabalhar?</div>
</div>";s:18:"questiontextformat";s:1:"1";s:15:"generalfeedback";s:217:"<div data-canvas-width="407.3821576666666">Se Clara trabalha em Arte, Flora trabalha em Programação (regra 3). Então Bia só pode trabalhar em Testes (regra 2), e Dino só pode trabalhar em Roteiro (regra 4).</div>";s:21:"generalfeedbackformat";s:1:"1";s:11:"defaultmark";d:1;s:7:"penalty";d:0.3333333;s:5:"qtype";s:11:"multichoice";s:6:"length";s:1:"1";s:5:"stamp";s:43:"programacao.ufca.edu.br+160719130737+ppNYxU";s:7:"version";s:43:"programacao.ufca.edu.br+160719130737+tr1uNr";s:6:"hidden";s:1:"0";s:11:"timecreated";s:10:"1468933657";s:12:"timemodified";s:10:"1468933657";s:9:"createdby";s:1:"2";s:10:"modifiedby";s:1:"2";s:9:"contextid";s:2:"29";s:7:"options";O:8:"stdClass":14:{s:2:"id";s:1:"7";s:10:"questionid";s:2:"36";s:6:"layout";s:1:"0";s:6:"single";s:1:"1";s:14:"shuffleanswers";s:1:"1";s:15:"correctfeedback";s:34:"<p>Sua resposta está correta.</p>";s:21:"correctfeedbackformat";s:1:"1";s:24:"partiallycorrectfeedback";s:47:"<p>Sua resposta está parcialmente correta.</p>";s:30:"partiallycorrectfeedbackformat";s:1:"1";s:17:"incorrectfeedback";s:36:"<p>Sua resposta está incorreta.</p>";s:23:"incorrectfeedbackformat";s:1:"1";s:15:"answernumbering";s:3:"abc";s:14:"shownumcorrect";s:1:"1";s:7:"answers";a:5:{i:31;O:8:"stdClass":7:{s:2:"id";s:2:"31";s:8:"question";s:2:"36";s:6:"answer";s:29:"<p>Programação e testes</p>";s:12:"answerformat";s:1:"1";s:8:"fraction";s:9:"0.0000000";s:8:"feedback";s:0:"";s:14:"feedbackformat";s:1:"1";}i:32;O:8:"stdClass":7:{s:2:"id";s:2:"32";s:8:"question";s:2:"36";s:6:"answer";s:28:"<p>Somente programação</p>";s:12:"answerformat";s:1:"1";s:8:"fraction";s:9:"0.0000000";s:8:"feedback";s:0:"";s:14:"feedbackformat";s:1:"1";}i:33;O:8:"stdClass":7:{s:2:"id";s:2:"33";s:8:"question";s:2:"36";s:6:"answer";s:21:"<p>Roteiro e sons</p>";s:12:"answerformat";s:1:"1";s:8:"fraction";s:9:"0.0000000";s:8:"feedback";s:0:"";s:14:"feedbackformat";s:1:"1";}i:34;O:8:"stdClass":7:{s:2:"id";s:2:"3